      <Action name="rescale" text="Stretch or S&amp;quash..." />
    </Menu>
    <Menu name="Transpose">
      <text>Trans&amp;pose</text>
      <Action name="transpose_up" text="&amp;Up a Semitone" shortcut="Up" />
      <Action name="transpose_down" text="&amp;Down a Semitone" shortcut="Down" />
