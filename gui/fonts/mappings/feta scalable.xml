	<symbol name="MUSICAL SYMBOL COMBINING STACCATISSIMO"
                font-id="0" code="94" inversion-code="95" glyph="54"/>
	<symbol name="MUSICAL SYMBOL COMBINING MARCATO"
                font-id="0" code="99" inversion-code="100" glyph="59"/>
	<symbol name="MUSICAL SYMBOL FERMATA"
	        font-id="0" code="89" inversion-code="90" glyph="43" inversion-glyph="44"/>
	<symbol name="MUSICAL SYMBOL TR" font-id="0" code="107" glyph="67"/>
	<symbol name="MUSICAL SYMBOL TURN" font-id="0" code="106" glyph="66"/>
