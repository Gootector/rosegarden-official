		  <action>
		    Selects all segments within the composition.
		  </action>
		</para>
	      </listitem>
	    </varlistentry
>  

	    <varlistentry id="commands-main-edit-add-tempo-change">
	      <term>
		<menuchoice>
		  <guimenu
>Edit</guimenu>
		  <guimenuitem
>Add Tempo Change...</guimenuitem>
		</menuchoice>
	      </term>
